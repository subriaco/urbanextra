<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="urbanextra_view_pos_pos_form" model="ir.ui.view">
        <field name="name">urbanextra.pos.order.form</field>
        <field name="model">pos.order</field>
        <field name="inherit_id" ref="point_of_sale.view_pos_pos_form"/>
        <field name="arch" type="xml">
            <field name="partner_id" position="after">
                   <field name="invoice_user_id" />               
            </field>   
        </field>
    </record>
    <record id="urbanextra_view_pos_order_tree" model="ir.ui.view">
        <field name="name">urbanextra.pos.order.tree</field>
        <field name="model">pos.order</field>
        <field name="inherit_id" ref="point_of_sale.view_pos_order_tree"/>
        <field name="arch" type="xml">
            <field name="partner_id" position="after">
                   <field name="invoice_user_id" optional="show"/>
            </field>
        </field>
    </record>
</odoo>
